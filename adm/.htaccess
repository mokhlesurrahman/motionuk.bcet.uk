SetEnv HTACCESS on
<IfModule mod_rewrite.c>
RewriteEngine On

#RewriteCond %{SERVER_PORT} 80
#RewriteRule ^(.*)$ https://motionuk.bcet.uk/adm/$1 [R=301,L]

RewriteBase /sites/lavishcon.com/motionuk.bcet.uk/adm/
